import java.io.*;
import java.math.*;
import java.text.*;
import java.util.*;
import java.util.regex.*;

public class Solution {


    private static final Scanner scanner = new Scanner(System.in);

    public static void main(String[] args) throws IOException {
       
            int n = scanner.nextInt();
            int[] a = new int[1000];
            int sum=0;
            for(int i=0;i<n;i++)
            {
                a[i] = scanner.nextInt();
            }
            for(int i=0;i<n;i++)
            {
                    sum=sum+a[i];
            }
        System.out.println(sum);
    }
}
